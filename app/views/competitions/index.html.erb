<h1> Competitions </h1>

<ul>
  <% @competitions.each do |competition| %>
    <li>
      <%= link_to competition.name, competition %>
      <%= link_to "Edit", edit_competition_path(competition) %>
      <%= link_to "Destroy", competition_path(competition),
        method: :delete,
        data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>
