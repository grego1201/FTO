<%= link_to "Matches", matches_path %>

<ul>
  <li> ID: <%= @match.id %> </li>
  <li> right_points: <%= @match.right_points %> </li>
  <li> left_points: <%= @match.left_points %> </li>
</ul>

<ul>
  <li> <%= link_to "Edit", edit_match_path(@match) %> </li>
  <li> <%= link_to "Destroy", match_path(@match),
    method: :delete,
    data: { confirm: 'Are you sure?' } %> </li>
</ul>
